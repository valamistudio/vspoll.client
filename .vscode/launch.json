{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Chrome: Attach",
        "type": "chrome",
        "request": "attach",
        "port": 9222,
        "urlFilter": "localhost:4200*",
        "webRoot": "${workspaceFolder}",
        "sourceMapPathOverrides": {
          "webpack:/*": "${webRoot}/*",
          "/./*": "${webRoot}/*",
          "/src/*": "${webRoot}/*",
          "/*": "*",
          "/./~/*": "${webRoot}/node_modules/*"
        }
      },
    ]
  }
